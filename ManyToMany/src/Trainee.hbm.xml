<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ncs.manytomany.Trainee" table="trainee">
		<id name="tid" type="int" column="tid">
		<generator class ="increment"></generator>	
		</id>
		<property name="tname">
			<column name="tname" />
		</property>
		
		<set name="techStack" table ="TraineeSkills" cascade="all">
		<key column="tid"></key>
		<many-to-many column="sid" class="com.ncs.manytomany.Skills"></many-to-many>
		</set>
		
	</class>
</hibernate-mapping>